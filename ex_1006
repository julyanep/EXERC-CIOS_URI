using System;
using System.Globalization;

class Program {
    static void Main(string[] args) {
        double a, b, c, media, mediap;

		a = double.Parse(Console.ReadLine());
		b = double.Parse(Console.ReadLine());
		c = double.Parse(Console.ReadLine());

		media = (a * 2) + (b * 3) + (c * 5);
		mediap = media / (2 + 3 + 5);

		Console.WriteLine("MEDIA = " + mediap.ToString("F1", CultureInfo.InvariantCulture));

		Console.ReadLine();
    }
}
